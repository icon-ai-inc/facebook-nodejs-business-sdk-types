#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/flow-to-ts/bin/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/flow-to-ts/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/flow-to-ts/bin/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/flow-to-ts/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules/@khanacademy/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/@khanacademy+flow-to-ts@0.5.2/node_modules:/Users/abhi/icon/bios/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@khanacademy/flow-to-ts/bin/flow-to-ts.js" "$@"
else
  exec node  "$basedir/../@khanacademy/flow-to-ts/bin/flow-to-ts.js" "$@"
fi
